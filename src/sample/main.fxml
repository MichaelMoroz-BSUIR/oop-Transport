<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox fx:id="root" minHeight="-Infinity" minWidth="-Infinity" prefWidth="700.0" spacing="10.0"
      style="-fx-background-color: #bba;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.Main">
    <children>
        <VBox spacing="10.0">
            <children>
                <ListView fx:id="listView" prefHeight="350.0" prefWidth="380.0">
                </ListView>
                <HBox alignment="CENTER" spacing="5.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#add" prefWidth="100.0" text="Add"/>
                        <ChoiceBox fx:id="choiceBox" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
                        <Button mnemonicParsing="false" onAction="#remove" prefWidth="100.0" text="Remove"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</HBox>
